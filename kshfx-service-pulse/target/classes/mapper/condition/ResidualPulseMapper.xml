<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.kshfx.pulse.condition.dao.ResidualPulseMapper">

    <select id="getResidualPulseList" resultType="com.kshfx.pulse.condition.bean.ResidualPulse">
        select *
        from TASK_RESIDUAL_PULSE
        <where>
            <if test="taskId != null and taskId != ''">
                and TASK_ID = #{taskId}
            </if>
            <if test="conditionId != null and conditionId != ''">
                and CONDITION_ID = #{conditionId}
            </if>
        </where>
        order by ID
    </select>

</mapper>